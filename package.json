    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "webpack": "^5.x.x",
    "webpack-cli": "^5.x.x",
    "webpack-dev-server": "^4.x.x"
  },
  "dependencies": {
    "@vercel/edge-config": "^1.4.0",
    "phaser": "^3.70.0"
