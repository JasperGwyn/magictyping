{
  "name": "phaser-version",
  "version": "1.0.0",
  "description": "Phaser version of Lupita Typing Game",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --progress",
    "dev": "cross-env NODE_ENV=development webpack serve --open --progress",
